<div class="time_info"
     tal:condition="start_time or end_time">
    <span tal:condition="start_time">
    	starts @ <span tal:content="dt_format(start_time)">1970-01-01 01:01</span>
	</span>
    <span tal:condition="start_time and end_time">&mdash;</span>
    <span tal:condition="end_time">
        ends @ <span tal:content="dt_format(end_time)">1970-01-01 01:01</span>
    </span>
</div>
