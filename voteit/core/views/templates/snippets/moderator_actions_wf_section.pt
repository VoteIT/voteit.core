<metal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.core">
<li><span class="menusection">${section_title}</span></li>
<li>
    <span class="readonly">
        <span class="iconpadding icon ${state_id}">
        ${api.tstring(state_id.title())}
        - (<span i18n:translate="">current</span>)
        </span>
    </span>
</li>
<tal:states tal:condition="states|False">
    <tal:iterate repeat="state states">
    <li>
        <a class="confirm-state"
           tal:attributes="href '%s%s' % (state_change_url, state.name)">
            <span class="iconpadding icon ${state.name}" tal:content="api.tstring(state.title)">Title</span>
        </a>
    </li>
    </tal:iterate>
</tal:states>
</metal:main>
