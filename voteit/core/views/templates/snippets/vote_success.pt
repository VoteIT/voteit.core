<tal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.core">
<div class="listing_block">
    <h3 i18n:translate="">Your vote has been registered!</h3>
    <p i18n:translate="">As long as the poll is open, you may change your vote. If you want to review it, just open this window again.</p>
    <p>&nbsp;</p>
    <center>
        <a href="${request.resource_url(context.__parent__, anchor = context.uid)}"
           id="close_vote_button" class="buttonize"
           i18n:translate="">Close</a>
    </center>
</div>
</tal:main>
