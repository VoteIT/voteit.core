	        <div>Remaining meeting time: <span tal:content="remaining_meeting_time">0</span></div>
        </fieldset>
	</div>
	<div id="content" metal:fill-slot="content">
	    <div class="column">
	        <h1>This has happened</h1>
            <ul>
            </ul>
	        
