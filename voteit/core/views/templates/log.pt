<metal:main i18n:domain="voteit.core">
	<tal:closed condition="log" repeat="entry log">
	    <ul>
	        <li>
	        	<a tal:attributes="href api.resource_url(entry, request)">
		        	<div tal:content="entry.title">Title</div>
		            <div tal:content="structure api.get_time_info(entry, request)">start time and end time macro</div>
		            <div tal:content="entry.get_field_value('summary')">Summary</div>
				</a>
	        </li>
	    </ul>
	</tal:closed>
	<tal:no_active condition="not log" i18n:translate="">
	    Nothing planned yet
	</tal:no_active>
</metal:main>
