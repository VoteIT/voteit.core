<tal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.core">
<span class="creators" tal:condition="users">
<tal:iterate repeat="user users">
	<a tal:condition="user"
	   tal:attributes="href resource_url(user, request)">
        <img tal:replace="structure user.get_image_tag()" />
	    <span tal:content="user.title">My fullname</span>
	</a><tal:sep tal:condition="not repeat['user'].end">|</tal:sep>
</tal:iterate>
</span>
</tal:main>
