<tal:main xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="voteit.core">
<div id="ai-proposals">
  <div class="panel panel-default portlet">
    <div class="panel-heading">
      <h4 class="panel-title" i18n:translate="">Proposals</h4>
    </div>
    <ul class="list-group">
      <tal:iterate repeat="obj [x for x in context.values() if x.type_name == 'Proposal']">
      <li class="list-group-item">
        <tal:creator replace="structure request.creators_info(obj.creators)"></tal:creator>
        ${obj.title}
         
        <tal:meta replace="structure view.render_view_group('metadata_listing', obj)"></tal:meta>
      </li>
      </tal:iterate>
    </ul>
  </div>
</div>
</tal:main>
