<tal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.core">
<h1 i18n:translate="">Poll result</h1>
<div tal:condition="result">
    <tal:iterate repeat="item result">
    	<div>
    		<strong>
    			<span tal:replace="item['percentage']">30%</span>
				(<span tal:replace="item['count']">3</span> vote)
			</strong>
			&mdash;
			<tal:proposal tal:define="proposal get_proposal_by_uid(item['uid']['proposal'])">
				<span tal:content="structure proposal.title">Title</span>
			</tal:proposal>
		</div>
    </tal:iterate>
</div>
</tal:main>