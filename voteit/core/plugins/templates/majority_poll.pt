<tal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.core">
<div tal:condition="result">
    <tal:iterate repeat="item result">
        <div>
            <span tal:replace="item['percentage']">30%</span>
            (<span tal:replace="item['count']">3</span> ${vote_text(item['count'])})
        </div>
        <tal:proposal
            define="brain api.get_metadata_for_query(uid=item['uid']['proposal'])[0]"
            replace="structure api.render_single_view_component(api.context, api.request, 'proposal', 'block', brain=brain)">
            Proposal
        </tal:proposal>
    </tal:iterate>
    <tal:iterate repeat="looser novotes">
        <div>
        <span>0%</span>
            (<span>0</span> ${vote_text(0)})
        </div>
        <tal:proposal
            define="brain api.get_metadata_for_query(uid=looser)[0]"
            replace="structure api.render_single_view_component(api.context, api.request, 'proposal', 'block', brain=brain)">
            Proposal
        </tal:proposal>
    </tal:iterate>
</div>
</tal:main>
