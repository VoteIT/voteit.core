<tal:main xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="voteit.core">
<div id="agenda-item-portlet" class="portlet">
  <div class="pull-right" tal:condition="request.is_moderator">
    <a class="btn btn-primary btn-sm" href="${request.resource_url(request.meeting, 'manage_agenda')}" i18n:translate="">Manage</a>
  </div>
  <h2 i18n:translate="">Agenda</h2>
  <div data-load-target="${load_url}">
      <span class="glyphicon glyphicon-refresh rotate-me"></span>
      Loading...
  </div>
</div>
</tal:main>
