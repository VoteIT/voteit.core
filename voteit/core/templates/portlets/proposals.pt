<tal:main xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="voteit.core">
<div id="ai-proposals">
  <div class="panel panel-primary portlet">
    <div class="panel-heading">
      <h4 class="panel-title">
        <span class="glyphicon glyphicon-exclamation-sign"></span>
        <span i18n:translate="">Proposals</span>
      </h4>
    </div>
    <div class="list-group" data-load-target="${load_url}">
      <div class="list-group-item">Loading...</div>
    </div>
    <div class="panel-footer">
      <div tal:replace="structure view.render_template('voteit.core:templates/portlets/inline_add_button_prop.pt')"></div>
    </div>
  </div>
</div>
</tal:main>
